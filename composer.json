{
	"name": "dieselfox1/finpress-core-installer",
	"description": "A custom installer to handle deploying FinPress with composer",
	"keywords": [
		"finpress"
	],
	"type": "composer-plugin",
	"license": "GPL-2.0-or-later",
	"minimum-stability": "dev",
	"prefer-stable": true,
	"authors": [
		{
			"name": "Devyn",
			"email": "devyntwilliam@gmail.com"
		}
	],
	"autoload": {
		"psr-0": {
			"dieselfox1\\Composer\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\dieselfox1\\Composer\\": "tests/"
		}
	},
	"extra": {
		"class": "dieselfox1\\Composer\\FinPressCorePlugin"
	},
	"require": {
		"composer-plugin-api": "^1.0 || ^2.0",
		"php": ">=5.6.0"
	},
	"require-dev": {
		"composer/composer": "^1.0 || ^2.0",
		"phpunit/phpunit": ">=5.7.27"
	},
	"conflict": {
		"composer/installers": "<1.0.6"
	},
	"scripts": {
		"test:phpunit": "phpunit",
		"test": [
			"@test:phpunit"
		]
	}
}
